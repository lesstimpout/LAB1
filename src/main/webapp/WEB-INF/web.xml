<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AuthorizationServlet</servlet-name>
        <servlet-class>servlets.AuthorizationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthorizationServlet</servlet-name>
        <url-pattern>/authorization</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SelectServlet</servlet-name>
        <servlet-class>crud.SelectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SelectServlet</servlet-name>
        <url-pattern>/select</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateAgentServlet</servlet-name>
        <servlet-class>crud.UpdateAgentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateAgentServlet</servlet-name>
        <url-pattern>/updateAgent</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteAgentServlet</servlet-name>
        <servlet-class>crud.DeleteAgentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteAgentServlet</servlet-name>
        <url-pattern>/deleteAgent</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateAgreementServlet</servlet-name>
        <servlet-class>crud.UpdateAgreementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateAgreementServlet</servlet-name>
        <url-pattern>/updateAgreement</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteAgreementServlet</servlet-name>
        <servlet-class>crud.DeleteAgreementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteAgreementServlet</servlet-name>
        <url-pattern>/deleteAgreement</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateClientServlet</servlet-name>
        <servlet-class>crud.UpdateClientServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateClientServlet</servlet-name>
        <url-pattern>/updateClient</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteClientServlet</servlet-name>
        <servlet-class>crud.DeleteClientServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteClientServlet</servlet-name>
        <url-pattern>/deleteClient</url-pattern>
    </servlet-mapping>
</web-app>